#include <stdio.h>
#include <stdlib.h>

// ALgoritmo, "Tabuada"
// Aluno: Alan Guedes Pereira
// Data: 13/09/2025
// Descrição: Compilador de Tabuada

int main()
{
    int numero, i;

    printf("Digite um número para ver a sua tabuada: ");
    scanf("%d",&numero);

    printf("Tabuada de %d: \n", numero);
    for(i=1;i<=10;i++){
        printf("%d x %d = %d \n", numero,i,(numero*i));
    }

    return 0;
}
